{
    "name" : " Minerva Training Amplify Tool",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Quick extension to solve a very annoying problem.",
    "content_scripts" : [
        {
            "js" : ["init.js"],
            "css": ["styles.css"],
            "matches" : ["*://www.minerva.edu/admissions/evaluate*"]
        },
        {
            "js": ["getAnswers.js"],
            "run_at": "document_start",
            "matches" : ["*://www.minerva.edu/*"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
      },
    "background": {
        "scripts": ["background.js", "convert-file.js"]
    },
    "permissions": [
        "*://www.minerva.edu/*", "tabs", "webNavigation", "downloads"
    ]
}