{
    "name" : " Minerva Training Amplify Tool",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Quick extension to solve a very annoying problem.",
    "content_scripts" : [
        {
            "js" : ["init.js"],
            "css": ["styles.css"],
            "matches" : ["*://www.minerva.edu/*"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "*://www.minerva.edu/*", "tabs", "webNavigation"
    ]
}